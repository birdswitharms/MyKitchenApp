<p><strong><h3>Log In</h3></strong></p>
<div class="form">
<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :name %> <br />
    <%= text_field_tag :name %>
  </p>
  <p>
     <%= label_tag :password %><br />
     <%= password_field_tag :password %>
  </p>
  <p>
    <%= submit_tag "Log In" %>
  </p>
<% end %>
</div>
